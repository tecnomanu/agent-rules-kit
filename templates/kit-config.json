					]
				}
			},
			"atomic": {
				"name": "Atomic Design",
				"pattern_rules": {
					"<root>/src/**/*.{ts,tsx,js,jsx}": [
						"stacks/react/architectures/atomic/atomic-architecture.md"
					]
				}
			},
			"feature-sliced": {
				"name": "Feature-Sliced Design",
				"pattern_rules": {
					"<root>/src/**/*.{ts,tsx,js,jsx}": [
						"stacks/react/architectures/feature-sliced/feature-sliced-architecture.md"
					]
				}
			}
		}
	},
	"vue": {
		"default_architecture": "composition",
		"version_ranges": {
			"2": {
				"name": "Vue 2",
				"range_name": "v2"
			},
			"3": {
				"name": "Vue 3",
				"range_name": "v3"
			}
		},
		"pattern_rules": {
			"<root>/src/components/**/*.{vue,ts,js}": [
				"stacks/vue/base/architecture-concepts.md"
			],
			"<root>/tests/**/*.{spec,test}.{ts,js}": [
				"stacks/vue/base/vue-testing.md"
			]
		},
		"architectures": {
			"composition": {
				"name": "Composition API",
				"pattern_rules": {
					"<root>/src/composables/**/*.{ts,js}": [
						"stacks/vue/architectures/composition/composables.md"
					]
				}
			},
			"options": {
				"name": "Options API",
				"pattern_rules": {
					"<root>/src/mixins/**/*.{ts,js}": [
